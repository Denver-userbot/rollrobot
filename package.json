    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.13.4",
    "istanbul": "^0.4.5",
    "jest": "^20.0.4",
    "lint-staged": "^4.1.3",
    "prettier": "^1.6.1",
    "telegram-test": "^0.1.5"
  },
  "engines": {
    "node": ">= 8.4.0",
    "npm": ">= 5.4.1",
    "yarn": ">= 1.0.0"
